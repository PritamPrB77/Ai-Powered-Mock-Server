GENERATION_PROVIDER=openrouter

# OpenRouter (primary provider)
OPENROUTER_ENABLED=true
OPENROUTER_API_KEY=
OPENROUTER_MODEL=openai/gpt-4o-mini
OPENROUTER_FALLBACK_ENABLED=false
OPENROUTER_HISTORY_TURNS=5
OPENROUTER_STRICT_GENERATION=true
OPENROUTER_ALLOW_PROVIDER_FALLBACKS=false

# Local Ollama (optional fallback)
OLLAMA_ENABLED=true
OLLAMA_URL=http://localhost:11434/api/generate
OLLAMA_MODEL=qwen3:5
OLLAMA_FALLBACK_ENABLED=true
OLLAMA_NUM_PREDICT=256
OLLAMA_TOP_K=20
OLLAMA_TOP_P=0.9
GENERATION_TEMPERATURE=0.85

# Optional local Seq2Seq
SEQ2SEQ_ENABLED=false
SEQ2SEQ_MODEL_NAME=google/flan-t5-base
